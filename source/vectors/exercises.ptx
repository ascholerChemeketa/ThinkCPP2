      <p>For example:
      </p>
      <pre>
Input: 1 2 3 4 5
Returns: 5 2 3 4 1

Input: 10 20 30
Output: 30 20 10
      </pre>
      </statement>
      <program interactive="activecode">
        <preamble visible="yes"><![CDATA[
#include <iostream>
#include <vector>
using namespace std;
]]>
        </preamble>
        <code><![CDATA[

    // write swapEnds
]]>
        </code>
        <tests visible="yes">
<![CDATA[
#define DOCTEST_CONFIG_IMPLEMENT_WITH_MAIN
#include "doctest.h"



TEST_CASE("swapEnds") {
  vector<int> vec1 = {1, 2, 3, 4};
  trimZeros(vec1);
  CHECK(vec1 == vector<int>({4, 2, 3, 1}));
  vector<int> vec2 = {100, 200, 300};
  trimZeros(vec2);
  CHECK(vec2 == vector<int>({300, 200, 100}));
  vector<int> vec3 = {10, 9, 8, 7, 6, 5, 4};
